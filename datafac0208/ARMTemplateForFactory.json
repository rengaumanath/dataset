{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafac0208"
		},
		"sinkGitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'sinkGitHub1'"
		},
		"sourceAzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourceAzureDataLakeStorage1'"
		},
		"sinkGitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "rengaumanath"
		},
		"sourceAzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage0208231.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/sourcesExcel1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "sourceAzureDataLakeStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "Original sheet",
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "Data Points for Summary.xlsx",
						"fileSystem": "practisefile"
					},
					"firstRowAsHeader": true
				},
				"schema": [
					{
						"name": "Target",
						"type": "String"
					},
					{
						"name": "Regions",
						"type": "String"
					},
					{
						"name": "Channel",
						"type": "String"
					},
					{
						"name": "2018W51",
						"type": "String"
					},
					{
						"name": "2018W52",
						"type": "String"
					},
					{
						"name": "2019W 1",
						"type": "String"
					},
					{
						"name": "2019W 2",
						"type": "String"
					},
					{
						"name": "2019W 3",
						"type": "String"
					},
					{
						"name": "2019W 4",
						"type": "String"
					},
					{
						"name": "Sum",
						"type": "String"
					},
					{
						"name": "Average",
						"type": "String"
					},
					{
						"name": "rat%{sum}",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/sourceAzureDataLakeStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/sinkGitHub1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "GitHub",
				"typeProperties": {
					"username": "[parameters('sinkGitHub1_properties_typeProperties_username')]",
					"credential": {
						"type": "SecureString",
						"value": "[parameters('sinkGitHub1_credential')]"
					},
					"encryptedCredential": "ew0KICAiVmVyc2lvbiI6ICIyMDE3LTExLTMwIiwNCiAgIlByb3RlY3Rpb25Nb2RlIjogIktleSIsDQogICJTZWNyZXRDb250ZW50VHlwZSI6ICJQbGFpbnRleHQiLA0KICAiQ3JlZGVudGlhbElkIjogIkRBVEFGQUNUT1JZQDAxQTlDNUUzLTkzQUQtNEFBQi05MEZFLTM3RjdCREVGODM1NF84MWUyNDA0Ni0xZDdmLTRlNzQtODcxYy00YzE2ZDlhMjUyMjQiDQp9"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/sourceAzureDataLakeStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('sourceAzureDataLakeStorage1_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('sourceAzureDataLakeStorage1_accountKey')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}