{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafac0208"
		},
		"sinkGitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'sinkGitHub1'"
		},
		"sourceAzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourceAzureDataLakeStorage1'"
		},
		"sinkGitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "rengaumanath"
		},
		"sourceAzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage0208231.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafac0208_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sinkGitHub1_credential": {
						"value": "[parameters('sinkGitHub1_credential')]"
					},
					"sourceAzureDataLakeStorage1_accountKey": {
						"value": "[parameters('sourceAzureDataLakeStorage1_accountKey')]"
					},
					"sinkGitHub1_properties_typeProperties_username": {
						"value": "[parameters('sinkGitHub1_properties_typeProperties_username')]"
					},
					"sourceAzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('sourceAzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}